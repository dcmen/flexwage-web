<!DOCTYPE html>
<html lang="en">

<head>
  <title><%= title %></title>
  <!-- [if lt IE 10]>
    <script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
    <script src="https://oss.maxcdn.com/libs/respond.js/1.4.2/respond.min.js"></script>
    <![endif] -->
  <!-- Meta -->
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0, user-scalable=0, minimal-ui">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="referrer" content="strict-origin-when-cross-origin">
  <!-- ico font -->
  <link rel="stylesheet" type="text/css" href="../../web-files/assets/icon/icofont/css/icofont.css">
  <!-- Data Table Css -->
  <link rel="stylesheet" type="text/css" href="../../web-files/bower_components/datatables.net-bs4/css/dataTables.bootstrap4.min.css">
  <link rel="stylesheet" type="text/css" href="../../web-files/assets/pages/data-table/css/buttons.dataTables.min.css">
  <link rel="stylesheet" type="text/css" href="../../web-files/bower_components/datatables.net-responsive-bs4/css/responsive.bootstrap4.min.css">
  <link rel="stylesheet" type="text/css" href="../../web-files/bower_components/bootstrap-daterangepicker/css/daterangepicker.css" />

  <!-- Favicon icon -->
  <link rel="icon" href="/../../web-files/assets/images/favicon.ico" type="image/x-icon">
  <!-- Google font-->
  <link href="https://fonts.googleapis.com/css?family=Inter:400,600,800" rel="stylesheet">
  <!-- Required Fremwork -->
  <link rel="stylesheet" type="text/css" href="/../../web-files/bower_components/bootstrap/css/bootstrap.min.css">
  <!-- feather Awesome -->
  <link rel="stylesheet" type="text/css" href="/../../web-files/assets/icon/feather/css/feather.css">
  <!-- Style.css -->
  <link rel="stylesheet" type="text/css" href="/../../web-files/assets/css/style.css">
  <link rel="stylesheet" type="text/css" href="/../../web-files/assets/css/jquery.mCustomScrollbar.css">
  <link rel="stylesheet" type="text/css" href="../../web-stylesheets/style.css">
  <!-- iconfont -->
  <link rel="stylesheet" type="text/css" href="/../../web-files/assets/icon/icofont/css/icofont.css">
  <!-- font-awesome -->
  <link rel="stylesheet" type="text/css" href="/../../web-files/assets/icon/font-awesome/css/font-awesome.min.css">
  <link rel="stylesheet" href="/../../web-files/assets/css/iziModal.min.css" />
  <!-- <script src="https://cdnjs.cloudflare.com/ajax/libs/dompurify/2.2.9/purify.min.js" crossorigin="anonymous" referrerpolicy="no-referrer"></script> -->
  <link href="/../../web-files/assets/css/select2.min.css" rel="stylesheet" />
  <link href="/../../web-files/assets/css/style-custom.css" rel="stylesheet" />
  <!-- Bootstrap Form Helpers -->
  <link href="../../web-files/bower_components/formhelpers/bootstrap-formhelpers.css" rel="stylesheet" media="screen">
  <link rel="stylesheet" type="text/css" href="../../web-files/bower_components/formhelpers/daterangepicker.css" />
  <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/cropperjs/1.5.12/cropper.min.css" />
</head>

<body>
  <% include ../layouts/preloader %>
  <div id="jsLoader">
    <div style="top: 40%; left: 0px;" class="loader">
      <div class="loader-chiled"></div>
    </div>
    <% if (!company.is_first_synced) { %>
    <p class="syncing-text">Syncing data...</p>
    <% } %>
  </div>
  <div id="pcoded" class="pcoded">
    <div class="pcoded-overlay-box"></div>
    <div class="pcoded-container navbar-wrapper">
      <% include ../layouts/navbar %>
      <div class="pcoded-main-container">
        <div class="pcoded-wrapper">
          <% include ../layouts/leftbar %>
          <!-- Toast -->
          <div id='jsErr' class="toast-err">
            <p>Connection timeout.</p>
            <div class="relative w-full sm:w-1/2 bg-gray-200">
              <div style="width: 100%" class="absolute top-0 h-4 shim-red"></div>
            </div>
          </div>
          <% for(var i=0; i < 5; i++) {%>
          <div id='jsErr<%= i %>' class="toast-err">
            <p>Connection timeout.</p>
            <div class="relative w-full sm:w-1/2 bg-gray-200">
              <div style="width: 100%" class="absolute top-0 h-4 shim-red"></div>
            </div>
          </div>
          <% } %>
          <!-- End toast -->
          <div class="pcoded-content">
            <div class="pcoded-inner-content">
              <!-- Main-body start -->
              <div class="main-body">
                <div class="page-wrapper">
                  <input hidden type="text" name="key" value="<%= key %>">
                  <input hidden type="text" name="role" value="<%= role %>">
                  <input hidden type="text" name="deductionRePaType" value="<%= company.deduction_repayment_type %>">
                  <input name="lenderLinkCompany" hidden value="<%= JSON.stringify(lenderLinkCompany[0]) %>" />
                  <input hidden type="text" name="isEmployer" value="<%= isEmployer %>">
                  <input hidden type="text" name="payrollSystem" value="<%= JSON.stringify(tokenPayroll) %>">
                  <!-- Page-body start -->
                  <div class="page-body">
                    <% if(errorMessage) { %>
                    <div class="alert alert-danger">
                      <button type="button" class="close" data-dismiss="alert" aria-label="Close">
                        <i class="icofont icofont-close-line-circled"></i>
                      </button>
                      <%= errorMessage %>
                    </div>
                    <% } %>
                    <div id="jsShowContentPage" class="row">
                      <div class="col-lg-12">
                        <!-- check connect system -->
                        <div class="check-connect-system text-right mb-2">
                          <% if (company.is_active != 2) { %>
                          <button class="btn btn-outline-info btn-sync-payroll mr-2 <%= !company.is_fail_system_refresh_token && (['XERO', 'DEPUTY', 'KEYPAY', 'RECKON', 'ASTUTE', 'HR3'].includes(system.code)) ? '' : 'disabled' %> ">
                            <i class="icofont icofont-spinner-alt-3"></i>
                            Sync Payroll
                          </button>
                          <% } %>

                          <button class="btn btn-reconnect-system <%= company.is_fail_system_refresh_token ? 'btn-outline-danger' : 'btn-outline-success' %>">
                            <div class="d-flex">
                              <div class="circle <%= company.is_fail_system_refresh_token ? 'normal-circle' : 'pulsating-circle'%>">
                              </div>
                              <span><%= company.is_fail_system_refresh_token ? 'Disconnected' : 'Connected' %></span>
                            </div>
                          </button>
                        </div>
                        <!-- tab content start -->
                        <div class="tab-content">
                          <!-- tab panel personal start -->
                          <div class="tab-pane active" id="personal" role="tabpanel">
                            <!-- personal card start -->
                            <div class="card card--custom">
                              <div class="card-block">
                                <div class="view-info">
                                  <div class="row">
                                    <div class="col-lg-12">
                                      <div class="row">
                                        <div class="card-header col-lg-8 p-0">
                                          <ul class="nav nav-tabs">
                                            <li class="nav-item">
                                              <a class="nav-link btn active" id='jsWelcome'><b>Welcome</b></a>
                                            </li>
                                            <li class="nav-item">
                                              <a class="nav-link btn" id='jsCompany'><b>Company</b></a>
                                            </li>
                                            <% if(role == 'Admin') { %>
                                            <li class="nav-item">
                                              <a class="nav-link btn" id='jsKYC'><b>KYC</b></a>
                                            </li>
                                            <% } %>
                                            <li class="nav-item">
                                              <a class="nav-link btn" id='jsFinancial'>
                                                <b>Financial</b>
                                                <i class="feather <%= company.is_financial_setup ? 'icon-check-circle' : 'icon-alert-circle' %>"></i>
                                              </a>
                                            </li>
                                            <li class="nav-item">
                                              <a class="nav-link btn" id='jsPayroll'>
                                                <b>Payroll</b>
                                                <i class="feather <%= company.is_payroll_setup ? 'icon-check-circle' : 'icon-alert-circle' %>"></i>
                                              </a>
                                            </li>
                                            <li class="nav-item">
                                              <a class="nav-link btn" id='jsMonoova'>
                                                <b>Banking</b>
                                              </a>
                                            </li>
                                            <li class="nav-item">
                                              <a class="nav-link btn" id='chatSetup'><b>
                                                  Chat</b></a>
                                            </li>
                                          </ul>

                                        </div>
                                        <div class="nav-tabs col-lg-4 d-flex justify-content-end align-items-center ">
                                          <b class="last-sync" style="color: red;">
                                            Last Sync:
                                            <%= company.last_synced ? moment(new Date(company.last_synced)).format('h:mm A DD/MM/YYYY') : 'N/A' %>
                                          </b>
                                        </div>
                                      </div>

                                      <div id="jsWelcomeDiv" class="general-info">
                                        <input type="hidden" name="company" value="<%= companyJSON %>">
                                        <input type="hidden" name="systemCompanyId" value="<%= company.system_company_id %>">
                                        <input type="hidden" name="urlApi" value="<%= urlApi %>">
                                        <input type="hidden" id="urlKeyPay" value="<%= urlSystem.urlKeyPay %>">
                                        <input type="hidden" id="urlXeRo" value="<%= urlSystem.urlXero %>">
                                        <input type="hidden" id="urlDeputy" value="<%= urlSystem.urlDeputy %>">
                                        <input type="hidden" name="systemCode" value="<%= system.code %>">
                                        <input type="hidden" name="role" value="<%= role %>">
                                        <input type="hidden" name="companyBrands" value="<%= JSON.stringify(companyBrands) %>">
                                        <input type='hidden' id="urlReckon" value="<%= urlSystem.urlReckon %>">
                                        <div class="text-welcome pt-3">
                                          <p>
                                            <span class="font-weight-bold"><%= company.company_name %>,
                                            </span>
                                            <br>
                                            Welcome to your company portal -
                                            one place to manage your CashD
                                            experience.
                                            If you have any issues, please
                                            email <a style="color: #305ed3" href="mailto:support@cashd.com.au">support@cashd.com.au.</a>
                                            <br>
                                            Enjoy using CashD
                                            <br>
                                            <br>
                                            The CashD team.
                                          </p>
                                        </div>
                                        <div class="dashboard-company pt-5">
                                          <div class="row">
                                            <!-- task, page, download counter  start -->
                                            <div class="col-xl-3 col-md-3">
                                              <div class="card bg-c-blue text-white">
                                                <div class="card-block">
                                                  <div class="row align-items-center">
                                                    <div class="col">
                                                      <p class="m-b-5">
                                                        Registered
                                                      </p>
                                                      <h4 class="m-b-0">
                                                        <%= totalRegistered %>
                                                      </h4>
                                                    </div>
                                                    <div class="col col-auto text-right">
                                                      <i class="feather icon-user-check f-50 text-c-blue"></i>
                                                    </div>
                                                  </div>
                                                </div>
                                              </div>
                                            </div>
                                            <div class="col-xl-3 col-md-3">
                                              <div class="card bg-c-green text-white">
                                                <div class="card-block">
                                                  <div class="row align-items-center">
                                                    <div class="col">
                                                      <p class="m-b-5">
                                                        Unregistered
                                                      </p>
                                                      <h4 id="jsTotalUnregistered" class="m-b-0">
                                                        <%= totalUnregistered %>
                                                      </h4>
                                                    </div>
                                                    <div class="col col-auto text-right">
                                                      <i class="feather icon-user-plus f-50 text-c-green"></i>
                                                    </div>
                                                  </div>
                                                </div>
                                              </div>
                                            </div>
                                            <div class="col-xl-3 col-md-3">
                                              <div class="card bg-c-pink text-white">
                                                <div class="card-block">
                                                  <div class="row align-items-center">
                                                    <div class="col">
                                                      <p class="m-b-5">
                                                        Unaccepted
                                                        Invitations
                                                      </p>
                                                      <h4 class="m-b-0 txt-total-invitation">
                                                        <%= totalInvitation %>
                                                      </h4>
                                                    </div>
                                                    <div class="col col-auto text-right">
                                                      <i class="icofont icofont-paper-plane f-50 text-c-pink"></i>
                                                    </div>
                                                  </div>
                                                </div>
                                              </div>
                                            </div>
                                            <div class="col-xl-3 col-md-3">
                                              <div class="card bg-c-yellow text-white">
                                                <div class="card-block">
                                                  <div class="row align-items-center">
                                                    <div class="col">
                                                      <p class="m-b-5">
                                                        Running
                                                        Total
                                                      </p>
                                                      <h4 class="m-b-0 txtRun">
                                                      </h4>
                                                    </div>
                                                    <div class="col col-auto text-right">
                                                      <i class="icofont icofont-money f-50 text-c-yellow"></i>
                                                    </div>
                                                  </div>
                                                </div>
                                              </div>
                                            </div>
                                          </div>
                                        </div>
                                      </div>

                                      <div id="jsCompanyDiv" class="general-info hiden">
                                        <div class="row">
                                          <div class="col-lg-12 col-xl-6">
                                            <div class="table-responsive">
                                              <table class="table m-0">
                                                <tbody>
                                                  <tr>
                                                    <th scope="row" style="border: none;">
                                                      Company Name
                                                    </th>
                                                    <td style="border: none;" id="jsNameCompany">
                                                      <%=company.company_name?company.company_name:"N/A"%>
                                                    </td>
                                                  </tr>
                                                  <tr>
                                                    <th scope="row">
                                                      System Name
                                                    </th>
                                                    <td><%= system.system_name %>
                                                    </td>
                                                  </tr>
                                                  <tr>
                                                    <th scope="row">
                                                      State</th>
                                                    <td><%=company.state?company.state:"N/A"%>
                                                    </td>
                                                  </tr>
                                                  <tr>
                                                    <th scope="row">
                                                      City</th>
                                                    <td class="ellipsis-txt">
                                                      <%=company.city?company.city:"N/A"%>
                                                    </td>
                                                  </tr>
                                                  <tr>
                                                    <th scope="row">
                                                      Suburb</th>
                                                    <td> <%=company.suburb?company.suburb:"N/A"%>
                                                    </td>
                                                  </tr>
                                                </tbody>
                                              </table>
                                            </div>
                                          </div>
                                          <!-- end of table col-lg-6 -->
                                          <div class="col-lg-12 col-xl-6">
                                            <div class="table-responsive">
                                              <table class="table m-0">
                                                <tbody>
                                                  <tr>
                                                    <th scope="row" style="border: none;">
                                                      Company
                                                      Address</th>
                                                    <td style="border: none;" class="ellipsis-txt">
                                                      <%=company.address?company.address:"N/A"%>
                                                    </td>
                                                  </tr>
                                                  <tr>
                                                    <th scope="row">
                                                      Postcode
                                                    </th>
                                                    <td><%=company.postcode?company.postcode:"N/A"%>
                                                    </td>
                                                  </tr>
                                                  <tr>
                                                    <th scope="row">
                                                      Company
                                                      Phone</th>
                                                    <td><%=company.phone_company?company.phone_company:"N/A"%>
                                                    </td>
                                                  </tr>
                                                  <tr>
                                                    <th scope="row">
                                                      Company
                                                      Email</th>
                                                    <td><%=company.email_company?company.email_company:"N/A"%>
                                                    </td>
                                                  </tr>
                                                </tbody>
                                              </table>
                                            </div>
                                          </div>
                                          <!-- end of table col-lg-6 -->
                                        </div>
                                      </div>

                                      <% let isShow = role !== 'Admin' ? 'disabled' : '' %>
                                      <div id='jsFinancialDiv' class="general-info hiden">
                                        <form style="min-height: 202px;">
                                          <input type="hidden" name="_id" value="<%= company._id %>">
                                          <input type="hidden" name="_csrf" id="_csrf" value="<%= csrfToken %>">
                                          <div class="row">
                                            <div class="col-xl-6 col-md-6 pt-3">
                                              <div class="row">
                                                <div class="col-6 d-flex align-items-center pl-4">
                                                  <b>Total #
                                                    employees</b>
                                                </div>
                                                <div class="col-6">
                                                  <input <%= isShow %> onkeypress='validateNumber(event)' placeholder="# employees" name="limit_number_of_employee" class="form-control total-input" type="text" value="<%= company.limit_number_of_employee ? company.limit_number_of_employee : 0 %>">
                                                  <small style='color: #fd0000;'></small>
                                                </div>
                                              </div>
                                            </div>
                                            <div class="col-xl-6 col-md-6 pt-3">
                                              <div class="row">
                                                <div class="col-6 d-flex align-items-center pl-4">
                                                  <b>Total Active
                                                    Employees</b>
                                                </div>
                                                <div class="col-6">
                                                  <input disabled placeholder="Active Employees" class="form-control total-input" type="text" value="<%= new Intl.NumberFormat(
                                                                                                       ).format(
                                                                                                               company.activeEmployees
                                                                                                       ) %>">
                                                </div>
                                              </div>
                                            </div>
                                            <div class="col-xl-6 col-md-6 pt-3">
                                              <div class="row">
                                                <div class="col-6 d-flex align-items-center pl-4">
                                                  <b>Total $ per
                                                    company</b>
                                                </div>
                                                <div class="col-6">
                                                  <input <%= isShow %> onkeypress='validateNumber(event)' placeholder="$ per company" name="limit_money" class="form-control total-input" type="text" value="<%= company.limit_money ? company.limit_money : 0 %>">
                                                  <small style='color: #fd0000;'></small>
                                                </div>
                                              </div>
                                            </div>
                                            <div class="col-xl-6 col-md-6 pt-3">
                                              <div class="row">
                                                <div class="col-6 d-flex align-items-center pl-4">
                                                  <b>Total $ drawn
                                                    this period</b>
                                                </div>
                                                <div class="col-6">
                                                  <input disabled placeholder="$ drawn this period" class="form-control total-input" type="text" value="<%= new Intl.NumberFormat(
                                                                                                               'en-US',
                                                                                                               { style: 'currency', currency: 'USD' }
                                                                                                       ).format(
                                                                                                               company.total_drawn_period
                                                                                                       ) %>">
                                                </div>
                                              </div>
                                            </div>
                                            <div class="col-xl-6 col-md-6 pt-3">
                                              <div class="row">
                                                <div class="col-6 d-flex align-items-center pl-4">
                                                  <b>Allowable %
                                                    drawdown</b>
                                                </div>
                                                <div class="col-6">
                                                  <input <%= isShow %> onkeypress='validateNumber(event)' placeholder="% drawdown" name="limit_allowable_percent_drawdown" class="form-control total-input" type="text" value="<%= company.limit_allowable_percent_drawdown ? company.limit_allowable_percent_drawdown : 0 %>">
                                                  <small style='color: #fd0000;'></small>
                                                </div>
                                              </div>
                                            </div>
                                            <div class="col-xl-6 col-md-6 pt-3">
                                              <div class="row">
                                                <div class="col-6 d-flex align-items-center pl-4">
                                                  <b>Balance
                                                    available</b>
                                                </div>
                                                <div class="col-6">
                                                  <input name="balance_available" disabled placeholder="Balance available" class="form-control total-input" type="text" value="<%= new Intl.NumberFormat(
                                                                                                                'en-US',
                                                                                                                { style: 'currency', currency: 'USD' }
                                                                                                        ).format(
                                                                                                                company.limit_money - company.total_drawn_period ? company.limit_money - company.total_drawn_period : 0
                                                                                                        )%>">
                                                </div>
                                              </div>
                                            </div>
                                            <div class="col-xl-6 col-md-6 pt-3">
                                              <div class="row">
                                                <div class="col-6 d-flex align-items-center pl-4">
                                                  <b>Transaction
                                                    Fee</b>
                                                </div>
                                                <div class="col-6">
                                                  <div class="row unit-custom">
                                                    <div class="col-8 pr-2">
                                                      <input <%= isShow %> onkeypress='validateNumber(event)' name="transaction_fee" placeholder="Transaction
                                                                                                                Fee" class="form-control total-input" type="text" value="<%= company.transaction_fee_value ? company.transaction_fee_value : 0 %>">
                                                      <small style='color: #fd0000;'></small>
                                                    </div>
                                                    <div class="col-4 pl-0">
                                                      <select <%= isShow %> id="selectedUnit" class=" form-control">
                                                        <option value="DOLLAR" <%= company.transaction_fee_type == 'DOLLAR' ? 'selected' : '' %>>
                                                          $
                                                        </option>
                                                        <option value="PERCENT" <%= company.transaction_fee_type == 'PERCENT' ? 'selected' : '' %>>
                                                          %
                                                        </option>
                                                      </select>
                                                    </div>
                                                  </div>
                                                </div>
                                              </div>
                                            </div>
                                            <div class="col-xl-6 col-md-6">
                                            </div>
                                            <div class="col-xl-6 col-md-6 pt-3">
                                              <div class="row">
                                                <div class="col-6 d-flex align-items-center pl-4">
                                                  <b>Minimum $
                                                    withdrawal</b>
                                                </div>
                                                <div class="col-6">
                                                  <input <%= isShow %> onkeypress='validateNumber(event)' name="min_withdrawal" placeholder="Minimum $ withdrawal" class="form-control total-input" type="text" value="<%= new Intl.NumberFormat(
                                                                                                                'en-US',
                                                                                                                { style: 'currency', currency: 'USD' }
                                                                                                        ).format(
                                                                                                                company.min_withdrawal ? company.min_withdrawal : 50
                                                                                                        ) %>">
                                                  <small style='color: #fd0000;'></small>
                                                </div>
                                              </div>
                                            </div>
                                            <div class="col-xl-6 col-md-6">
                                            </div>
                                              <!-- Float alert -->
                                              <% let isNewAccount = lenderLinkCompany[0]?.lenders[0]?.monoova_account_type === "NEW_ACCOUNT" ? '' : 'disabled' %>
                                              <div class="col-xl-6 col-md-6 pt-4">
                                                <div class="row">
                                                  <div class="col-md-12 pl-4">
                                                    <div class="col-md-12 text-border pb-4">
                                                      <span class="font-weight-bold text-u-border">Float alert</span>
                                                      <div class="row">
                                                        <div class="col-12">
                                                          <div class="row">
                                                            <div class="col-6 d-flex align-items-center"><b>Threshold amount</b></div>
                                                            <div class="col-6" style="padding-left: 10px;">
                                                              <input type="text" <%= isNewAccount %> onkeypress="validateNumber(event)" placeholder="" class="form-control total-input" name="threshold_amount" value="<%= new Intl.NumberFormat('en-US',{ style: 'currency', currency: 'USD' }).format(company.threshold_amount ? company.threshold_amount : 0) %>">
                                                              <small style='color: #fd0000;'></small>
                                                            </div>
                                                          </div>
                                                        </div>
                                                        <div class="col-12 mt-3">
                                                          <div class="row">
                                                            <div class="col-6 d-flex align-items-center"><b>Recipients</b></div>
                                                            <div class="col-6" style="padding-left: 10px;">
                                                              <div id="email-list" class="form-control total-input <%= isNewAccount %>"></div>
                                                              <div id="emails-input" class="form-control total-input hide">
                                                              </div>
                                                              <p class="log-err hide">Invalid email</p>
                                                            </div>
                                                          </div>
                                                        </div>
                                                      </div>
                                                    </div>
                                                  </div>
                                                </div>
                                              </div>
                                              <!-- Float alert end -->
