    $(document).ready(function() {
      const company = <%- companyJSON %>;
      var date = moment.utc(company.last_synced).format();
      $('.last-sync').html('Last Sync: ' + moment(date).local().format('h:mm A DD/MM/YYYY'));
      var canvas;
      let isSignature = false;
      let signature;
      var signaturePad;
      var cropper;

      $(".jsSignature").on("click", function(e) {
        if ($("#directDebitAuthorityModal").hasClass("show")) {} else {}
        canvas = document.getElementById('canvas');
        $("#jsShowSignature").modal("show");
        // function resizeCanvas() {
        //     var ratio = Math.max(window.devicePixelRatio || 1, 1);
        //     canvas.width = canvas.offsetWidth * ratio;
        //     canvas.height = canvas.offsetHeight * ratio;
        //     canvas.getContext("2d").scale(ratio, ratio);
        // }
        // window.onresize = resizeCanvas;
        // resizeCanvas();

        signaturePad = new SignaturePad(canvas, {
          backgroundColor: '#fff',
          minWidth: 1,
          maxWidth: 1
        });

      });

      $('#file-btn').on('change', function() {
        var file = this.files[0];
        const image = document.getElementById('previewImg');
        let uploadedImageType;
        if (file) {
          uploadedImageType = file.type;
          $("#canvas").addClass('hide');
          $('#previewImg').removeClass("hide");
          $(".undo").addClass("hide");
          $('#jsShowSignature .cropper-container').removeClass("hide");

          image.src = (window.URL ? URL : webkitURL).createObjectURL(file);
          if (cropper) {
            cropper.destroy();
          }
          cropper = new Cropper(image, {
            aspectRatio: 466 / 160,
            preview: '.img-preview',
            viewMode: 3,
            cropBoxResizable: false,
            minCropBoxWidth: 464,
            minCropBoxHeight: 160
          });
          image.value = null;
          cropper.cropped;
        } else {
          window.alert('Please choose an image file.');
        }

      });

      $(".clear").click(function() {
        if ($('#previewImg').hasClass("hide")) {
          signaturePad.clear();
        } else {
          $("#canvas").removeClass('hide');
          $('#previewImg').addClass("hide");
          $('.cropper-container').addClass("hide");
          $('#actual-btn').val('');
          $(".undo").removeClass("hide");
        }
      });

      $(".undo").click(function(event) {
        var data = signaturePad.toData();
        if (data) {
          data.pop(); // remove the last dot or line
          signaturePad.fromData(data);
        }
      });

      $(".save").click(function(event) {
        if (signaturePad.isEmpty() && $('#previewImg').hasClass("hide")) {
          alert("Please provide a signature first.");
        } else if ($('#previewImg').hasClass("hide")) {
          signature = signaturePad.toDataURL();
        } else {
          signature = cropper.getCroppedCanvas({
            width: 466,
            height: 160
          }).toDataURL('image/jpeg');
        }
        $("#jsShowSignature").modal('hide');
        $('.showSignature').attr('src', signature);
      });

      $("#canvas").on("mouseup", function(params) {
        isSignature = true;
      });

    });
  </script>
  <script src="../../web-files/assets/pages/company/direct-debit-request.js"></script>
  <script src="../../web-files/assets/pages/company/deduction-repayment.js"></script>
  <script src="../../web-files/assets/pages/data-table/js/employer-datatable.js"></script>
  <script src="../../web-files/assets/js/emailsEditor.bundle.js"></script>
</body>

</html>